Algoritmo "consumo"

Var
   distanciaPecorrida, combustivel, mediaConsumo : real
Inicio
   escreval("qual a distancia percorrida? ")
   leia(distanciaPecorrida)
   escreval("quanto de combustivel?")
   leia(combustivel)
   
   mediaConsumo <- distanciaPecorrida / combustivel
   
   escreval("seu consumo médio é de ", mediaConsumo:5:3)


Fimalgoritmo
